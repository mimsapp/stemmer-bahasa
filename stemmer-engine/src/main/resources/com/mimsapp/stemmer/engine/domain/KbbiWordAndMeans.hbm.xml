<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mimsapp.stemmer.engine.domain.KbbiWordAndMeans"
		table="KBBI_WORD_MEANS">
		<id name="id" type="int" column="ID">
			<generator class="native" />
		</id>
		<property name="word" column="WORD" type="string" />
		<property name="wordId" column="WORD_ID" type="int" />
		<property name="means" column="MEANS" type="string" />
		<property name="usages" column="USAGES" type="string" />
	</class>
</hibernate-mapping>